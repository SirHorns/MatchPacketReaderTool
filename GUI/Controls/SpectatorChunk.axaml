<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:GUI.ViewModels"
             x:DataType="vm:ChunkVM"
             x:Class="GUI.Controls.SpectatorChunk">
    
    <Border 
        Background="Gainsboro"
        BorderBrush="Black"
        BorderThickness="2"
        CornerRadius="3"
        Padding="10">
        <StackPanel>
            <TextBlock Text="Chunk ID"/>
        
            <TextBlock Text="PacketCount"/>
        
            <TextBlock Text="SerializedPackets"/>
            <TextBlock Text="SoftBad"/>
            <TextBlock Text="HardBad"/>
        </StackPanel>
    </Border>
    
</UserControl>
